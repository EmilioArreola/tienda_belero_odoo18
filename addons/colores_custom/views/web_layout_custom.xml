<?xml version="1.0" encoding="utf-8"?>
<!-- Este cÃ³digo se encarga de agregar el favicon para la parte del backend -->
<odoo>
    <data>
        <template id="custom_backend_favicon" inherit_id="web.layout" name="Custom Backend Favicon">
            <xpath expr="//link[@rel='shortcut icon']" position="replace">
                <link rel="shortcut icon" href="colores_custom/static/src/img/favicon.ico" type="image/x-icon"/>
            </xpath>
        </template>
    </data>
</odoo>
